#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

include /usr/share/dpkg/buildflags.mk

override_dh_auto_configure:
	dh_auto_configure -- LIBDIR="/usr/lib/$(DEB_HOST_MULTIARCH)" \
	                     SYSCONFDIR="/etc" 

override_dh_install:
	dh_install --fail-missing

%:
	dh $@ 
